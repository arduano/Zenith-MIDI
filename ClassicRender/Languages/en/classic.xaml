<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MSDNSample"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Palette picker-->
    <system:String x:Key="palettes_reload">Reload</system:String>
    <system:String x:Key="palettes_imageNote">Image needs to be a 16 (or 32 for gradients) pixels wide png, and any height. Transparency is supported.</system:String>
    <system:String x:Key="palettes_randomise">Randomise color order</system:String>
    
    <!--Other settings-->
    <system:String x:Key="firstNote">First Note</system:String>
    <system:String x:Key="lastNote">Last Note</system:String>
    <system:String x:Key="pianoHeight">Piano Height %</system:String>
    <system:String x:Key="noteScreenTime">Note screen time</system:String>
    <system:String x:Key="sameWidthNotes">Same Width Notes</system:String>
    <system:String x:Key="save">Save</system:String>
    <system:String x:Key="loadSaved">Load Saved</system:String>
    <system:String x:Key="setDefaults">Defaults</system:String>
    <system:String x:Key="blackNotesAbove">Draw black notes above (Warning: SLOWER!!)</system:String>

</ResourceDictionary>